    "_posts/生活/杂谈/岩石文.md": {
      "withStopwords": {
        "title": 1,
        "关于岩石文的资料整理与思考": 1,
        "date": 1,
        "update": 1,
        "abbrlink": 1,
        "rockscript": 1,
        "tags": 1,
        "岩石文": 1,
        "categories": 1,
        "学习": 1,
